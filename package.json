{  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev", 
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run build && npm run preview",
    "build": "cd frontend && npm run build",
    "test": "npm run test:routes && npm run test:security",
    "test:routes": "node scripts/verify-route-protection.js",
    "test:routes:auto": "node scripts/automated-route-tests.js",
    "test:security": "node scripts/security-audit.js",
    "audit:security": "npm run test:security",
    "audit:dependencies": "npm audit --recursive",
    "setup:testing": "npm install puppeteer --save-dev",
    "lint": "cd frontend && npm run lint",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^1.0.6",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.2.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "lucide-react": "^0.487.0",
    "mongoose": "^8.14.2",
    "next": "^15.3.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.6.6",
    "react-dom": "^19.1.0",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.9",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "puppeteer": "^21.0.0"
  }
}
